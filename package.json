{
  "name": "hid-pandemic-preparedness",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "lint:prettier": "npx prettier . --check",
    "lint:eslint": "npx eslint \"src/**/*.{js,ts,tsx,astro}\"",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "deploy-storybook-dev": "npm run build-storybook && bash ./scripts/deploy.sh storybook-static/ hid-ppt-storybook-dev",
    "deploy-storybook-main": "npm run build-storybook && bash ./scripts/deploy.sh storybook-static/ hid-ppt-storybook-main",
    "deploy-app-dev": "npm run build && bash ./scripts/deploy.sh dist/ hid-ppt-app-dev",
    "deploy-app-main": "npm run build && bash ./scripts/deploy.sh dist/ hid-ppt-app-main",
    "deploy-app-preview": "npm run build && bash ./scripts/deployPreview.sh",
    "build-bootstrap-css": "cd src/styles/bootstrap-5.3.8/ && npm ci && npm run css-compile",
    "build-bootstrap-js": "cd src/styles/bootstrap-5.3.8/ && npm ci && npm run js-compile",
    "sync-data-with-storyblok": "node scripts/sync-data-with-storyblok.js"
  },
  "dependencies": {
    "@astrojs/node": "^9.4.4",
    "@astrojs/react": "^4.3.0",
    "@fontsource-variable/open-sans": "^5.2.6",
    "@remixicon/react": "^4.6.0",
    "@storyblok/astro": "^7.3.3",
    "@types/sanitize-html": "^2.16.0",
    "astro": "^5.11.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "lodash": "^4.17.21",
    "marked": "^16.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sanitize-html": "^2.17.0",
    "sentence-case": "^3.0.4",
    "serverless-http": "^4.0.0",
    "storyblok-js-client": "^7.1.5",
    "swiper": "^12.0.2",
    "uuid": "^13.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@storybook/react-vite": "^9.0.17",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.32.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.89.2",
    "storybook": "^9.0.17",
    "vitest": "^3.2.4"
  }
